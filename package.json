{
  "name": "assemble-select-files",
  "description": "Assemble plugin that adds a `.selectFiles` method to the instance, for reading in a glob of files and prompting the user to select the files they want to write to the file system.",
  "version": "0.1.0",
  "homepage": "https://github.com/assemble/assemble-select-files",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "assemble/assemble-select-files",
  "bugs": {
    "url": "https://github.com/assemble/assemble-select-files/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "assemble-select-views": "^0.1.5",
    "extend-shallow": "^2.0.1"
  },
  "devDependencies": {
    "assemble": "^0.11.0",
    "assemble-core": "^0.17.2",
    "assemble-loader": "^0.4.0",
    "base-fs-conflicts": "^0.1.6",
    "base-fs-rename": "^0.1.2",
    "base-questions": "^0.6.0",
    "delete": "^0.3.2",
    "engine-handlebars": "^0.8.0",
    "fs-exists-sync": "^0.1.0",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.4.5"
  },
  "keywords": [
    "assemble",
    "files",
    "select"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "assemble-select-views",
      "list": [
        "assemble",
        "templates",
        "assemble-select-views",
        "generate"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
